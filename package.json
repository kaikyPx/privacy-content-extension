{
  "name": "privacy-content-extension",
  "version": "1.0.0",
  "description": "A Chrome extension that allows you to visually modify statistics card data on web pages, ideal for demonstrations and portfolios of account managers",
  "main": "content.js",
  "scripts": {
    "build": "web-ext build --source-dir=. --artifacts-dir=dist",
    "lint": "web-ext lint",
    "start": "web-ext run --source-dir=.",
    "test": "echo \"No tests specified\" && exit 0",
    "package": "web-ext build --source-dir=. --artifacts-dir=dist --filename=privacy-data-modifier.zip"
  },
  "keywords": [
    "chrome-extension",
    "privacy",
    "data-modifier",
    "statistics",
    "portfolio",
    "demo"
  ],
  "author": "Kaiky <kaikyg17@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "web-ext": "^7.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaikyPx/privacy-content-extension.git"
  },
  "bugs": {
    "url": "https://github.com/kaikyPx/privacy-content-extension/issues"
  },
  "homepage": "https://github.com/kaikyPx/privacy-content-extension#readme",
  "engines": {
    "node": ">=16.0.0"
  }
}
